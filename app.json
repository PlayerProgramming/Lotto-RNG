{
  "expo": {
    "name": "LottoRNG",
    "slug": "lotto-rng",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./source/assets/lottornglogo.png",
    "splash": {
      "image": "./source/assets/lottornglogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./source/assets/lottornglogo.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./source/assets/lottornglogo.png"
    },
    "description": "Small React Native project to provide random 7 numbers for Lottos.",
    "githubUrl": "https://github.com/PlayerProgramming/Lotto-RNG"
  }
}
